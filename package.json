{
  "name": "recipe-manager",
  "version": "1.0.0",
  "description": "Recipe Manager",
  "main": "server.js",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.20.0",
    "firebase-admin": "^9.1.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "require": "^2.4.20",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'yumyum/*'\" \"npm run client\"",
    "client": "cd yumyum && npm run start",
    "install": "cd yumyum && npm install",
    "build": "cd yumyum && npm run build",  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chindowns/recipe-manager.git"
  },
  "keywords": [
    "Node",
    "MySQL",
    "Express",
    "React"
  ],
  "author": "Michael Downs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chindowns/recipe-manager/issues"
  },
  "homepage": "https://github.com/chindowns/recipe-manager#readme"
}
